{
  "version": 2,
  "builds": [
    {
      "src": "gestao-comercial-ui/dist/gestao-comercial-ui/**/*",
      "use": "@vercel/static"
    }
  ],
  "outputDirectory": "gestao-comercial-ui/dist/gestao-comercial-ui",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
